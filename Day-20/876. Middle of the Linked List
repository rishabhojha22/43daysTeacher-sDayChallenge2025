class Solution {
    public ListNode middleNode(ListNode head) {
        int len=0;
        ListNode temp=head;

        while(temp!=null){
            len++;
            temp=temp.next;
        }

        temp=head;
        len/=2;
        len++;

        while(len!=-1){
            len--;
            if(len==0){
                return temp;
            }
            temp=temp.next;
        }
        return head;
    }
}
