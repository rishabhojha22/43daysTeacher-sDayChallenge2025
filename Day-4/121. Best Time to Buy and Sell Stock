class Solution {
    public int maxProfit(int[] prices) {

        int maxProfit=0;
        int minEle=Integer.MAX_VALUE;

        for(int i=0;i<prices.length;i++){
            minEle=Math.min(minEle,prices[i]);
            maxProfit=Math.max(maxProfit,prices[i]-minEle);
        }
        return maxProfit;
    }
}
