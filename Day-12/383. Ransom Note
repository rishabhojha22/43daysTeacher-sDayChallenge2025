class Solution {
    public boolean canConstruct(String ransomNote, String magazine) {
        int arr1[]=new int[26];
        int arr2[]=new int[26];

        int n1=ransomNote.length();
        int n2=magazine.length();

        for(int i=0;i<n1;i++){
            int curr=ransomNote.charAt(i)-'a';
            arr1[curr]++;
        }
        for(int i=0;i<n2;i++){
            int curr=magazine.charAt(i)-'a';
            arr2[curr]++;
        }

        for(int i=0;i<26;i++){
            if(arr1[i]>arr2[i]){
                return false;
            }
        }
        return true;
    }
}
